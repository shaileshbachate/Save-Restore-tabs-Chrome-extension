{
    "manifest_version": 3,
    "name": "Save and Restore Tabs",
    "version": "2.0",
    "description": "Saves open tabs in tab groups and restores them later",
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon128.png"
    }
}
